{
    "demoName": "08_nested-collapsed",
    "nodeType": "lane",
    "testCases": [
        {
            "name": "Nested Collapsed Lane Rendering",
            "description": "Test that nested lane structure with collapsed sub-lane renders correctly",
            "type": "visual",
            "expected": {
                "nodeCount": 7,
                "nodeTypes": ["lane", "rect"],
                "mainLaneVisible": true,
                "expandedSubLaneVisible": true,
                "collapsedSubLaneVisible": true,
                "expandedChildrenVisible": true,
                "collapsedChildrenHidden": true
            }
        },
        {
            "name": "Collapsed State Management",
            "description": "Test that collapsed state is properly managed in nested structure",
            "type": "structural",
            "expected": {
                "hasMetadata": true,
                "hasSettings": true,
                "hasNodes": true,
                "hasMainLane": true,
                "hasSubLanes": true,
                "hasCollapsedState": true,
                "properHierarchy": true
            }
        },
        {
            "name": "Mixed Layout Behavior",
            "description": "Test that layout works correctly with mixed expanded/collapsed states",
            "type": "layout",
            "expected": {
                "verticalStacking": true,
                "horizontalCentering": true,
                "properSpacing": true,
                "collapsedStateRespected": true,
                "expandedStateRespected": true
            }
        }
    ],
    "performance": {
        "maxRenderTime": 1000,
        "maxMemoryUsage": 50
    }
}
